<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_light_white"
    android:orientation="vertical" >
        <TextView 
            android:id="@+id/charge_finish_title" 
             android:layout_width="match_parent"
             android:layout_height="44dp"
             android:gravity="center"
             android:text="充电结束"
             android:textColor="@color/common_white"
             android:textSize="18sp"
            android:background="@color/common_orange"
            />
     <LinearLayout 
         android:id="@+id/charge_finish_total_money_tag"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_margin="30dp"
         android:layout_below="@id/charge_finish_title"
         android:background="@color/common_white"
         android:orientation="vertical"
         >
      <TextView 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="充电已完成，本次消费金额为:"
        android:layout_marginTop="20dp"
        android:textColor="@color/common_black"
        android:textSize="18sp"
        android:textStyle="bold"
        />
      <TextView 
        android:id="@+id/charge_finish_total_money"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="26.00元"
        android:textColor="@color/common_orange"
        android:textSize="24sp"
        android:textStyle="bold"
        />
     </LinearLayout>   
     
    <TextView 
        android:id="@+id/charge_finish_ordernum_tag"
        android:layout_below="@id/charge_finish_total_money_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginLeft="30dp"
       	android:layout_marginRight="10dp"
        android:text="订单编号:"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_ordernum"
        android:layout_below="@id/charge_finish_total_money_tag"
        android:layout_toRightOf="@id/charge_finish_ordernum_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
      <TextView 
        android:id="@+id/charge_finish_time_tag"
        android:layout_below="@id/charge_finish_ordernum_tag"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginLeft="30dp"
       	android:layout_marginRight="10dp"
        android:text="充电时间："
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_time"
        android:layout_marginTop="20dp"
        android:layout_below="@id/charge_finish_ordernum_tag"
        android:layout_toRightOf="@id/charge_finish_time_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_power_tag"
        android:layout_below="@id/charge_finish_time"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginLeft="30dp"
       	android:layout_marginRight="10dp"
        android:text="充电电量："
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_power"
        android:layout_marginTop="20dp"
        android:layout_below="@id/charge_finish_time"
        android:layout_toRightOf="@id/charge_finish_power_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
      <TextView 
        android:id="@+id/charge_finish_price_tag"
        android:layout_below="@id/charge_finish_power_tag"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginLeft="30dp"
       	android:layout_marginRight="10dp"
        android:text="充电电费："
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_price"
        android:layout_marginTop="20dp"
        android:layout_below="@id/charge_finish_power_tag"
        android:layout_toRightOf="@id/charge_finish_price_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
      <TextView 
        android:id="@+id/charge_finish_serverprice_tag"
        android:layout_below="@id/charge_finish_price_tag"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginLeft="30dp"
       	android:layout_marginRight="10dp"
        android:text="充电服务费:"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_serverprice"
        android:layout_marginTop="20dp"
        android:layout_below="@id/charge_finish_price_tag"
        android:layout_toRightOf="@id/charge_finish_serverprice_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_soc_tag"
        android:layout_below="@id/charge_finish_serverprice_tag"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginLeft="30dp"
       	android:layout_marginRight="10dp"
        android:text="电池SOC:"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
     <TextView 
        android:id="@+id/charge_finish_soc"
        android:layout_marginTop="20dp"
        android:layout_below="@id/charge_finish_serverprice_tag"
        android:layout_toRightOf="@id/charge_finish_soc_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/common_black"
        android:textStyle="bold"
        android:textSize="14sp"
        />
       <TextView 
        android:id="@+id/charge_finish_complete"
        android:layout_below="@id/charge_finish_soc"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginLeft="30dp"
        android:text="  完成  "
        android:textColor="@color/common_white"
        android:textSize="15sp"
        android:paddingLeft="40dp"
        android:paddingRight="40dp"
        android:background="@drawable/popup_select_shape_confirm"
        />
       <TextView 
        android:id="@+id/charge_finish_comment"
        android:layout_below="@id/charge_finish_soc"
        android:layout_marginTop="20dp"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       	android:layout_marginRight="30dp"
        android:text="去评价"
        android:textColor="@color/common_white"
        android:textSize="15sp"
        android:paddingLeft="40dp"
        android:paddingRight="40dp"
        android:background="@drawable/popup_select_shape_confirm"
        />

</RelativeLayout>
